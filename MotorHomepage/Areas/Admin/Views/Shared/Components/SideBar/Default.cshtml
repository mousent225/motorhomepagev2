@model List<SysMenuViewModel>
<!-- BEGIN MENUBAR-->
<div id="menubar" class="menubar-inverse ">
	<div class="menubar-fixed-panel">
		<div>
			<a class="btn btn-icon-toggle btn-default menubar-toggle" data-toggle="menubar" href="javascript:void(0);">
				<i class="fa fa-bars"></i>
			</a>
		</div>
		<div class="expanded">
			<a href="../../html/dashboards/dashboard.html">
				<span class="text-lg text-bold text-primary ">MATERIAL&nbsp;ADMIN</span>
			</a>
		</div>
	</div>
	<div class="menubar-scroll-panel">

		<!-- BEGIN MAIN MENU -->
		<ul id="main-menu" class="gui-controls">

			<!-- BEGIN DASHBOARD -->
			<li>
				<a href="../../html/dashboards/dashboard.html" class="active">
					<div class="gui-icon"><i class="md md-home"></i></div>
					<span class="title">Dashboard</span>
				</a>
			</li><!--end /menu-li -->
			<!-- END DASHBOARD -->

			@foreach (var item in Model.Where(x => string.IsNullOrEmpty(x.ParentId)))
			{
				@if (Model.Any(x => x.ParentId == item.Id))
				{
					<li class="gui-folder">
						<a>
							<div class="gui-icon"><i class="fa @item.IconCss"></i></div>
							<span class="title">@item.Name</span>
						</a>
						<!--start submenu -->
						<ul>
							@foreach (var jtem in Model.Where(x => x.ParentId == item.Id))
							{
								<li><a href="@jtem.URL"><span class="title">@jtem.Name</span></a></li>
							}
						</ul><!--end /submenu -->
					</li>
				}
				else
				{
					<li>
						<a href="@item.URL">
							<div class="gui-icon"><i class="fa @item.IconCss"></i></div>
							<span class="title">@item.Name</span>
						</a>
					</li>
				}
			}

		</ul><!--end .main-menu -->
		<!-- END MAIN MENU -->

		<div class="menubar-foot-panel">
			<small class="no-linebreak hidden-folded">
				<span class="opacity-75">Copyright &copy; 2014</span> <strong>CodeCovers</strong>
			</small>
		</div>
	</div><!--end .menubar-scroll-panel-->
</div>
<!--end #menubar-->
<!-- END MENUBAR -->